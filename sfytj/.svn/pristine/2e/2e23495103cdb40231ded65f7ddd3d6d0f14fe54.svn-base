.wsys_list2{
  .wsys_head {
      width: 100%;
      height: 40px;
      border-bottom: 2px solid gray;
      .wsys_title{
        font-weight: 500;
        line-height: 27px;
        color: #000;
        font-size: 30px;
      }
  } 
  .ms{              /*民事起诉状*/
    font-weight: 500;
    color: #b6b6b6;
    overflow-y: scroll;
    overflow-x: hidden;
    max-height: 469px;
    padding-right: 30px;
    margin-top: 10px;
    .title{
      text-align: center; /*标题大小颜色*/
      font-size: 28px;
      font-weight: 700;
    }
    .yg{
      margin-top: 20px;
      .bg{
        color: black;
      }
    }
    /*中间小标题*/
    .t1{
      text-align: center;
    }
    .t2{
      margin-left: 48px;
    }
    .t4{
      float: right;
      margin-right: 48px;
    }
    .t5{
      float: right;
      margin-top: 36px;
      margin-right: -105px;
    }
    li{
      ul{
        color:#b6b6b6;
      }
    }
  }
  .btn_row{
    padding:0px;
  }
}