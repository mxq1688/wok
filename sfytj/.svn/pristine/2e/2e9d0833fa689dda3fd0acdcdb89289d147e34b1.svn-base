		@include center-block;
		font-size: 30px;
		height: 530px;
		padding: 30px 226px;
	}

	.bzxrname_title{
		line-height:62px;
		.star{
		    font-size: 30px;
    		color: red;
    		font-weight: bold;
		}
	}

	.bzxrname_text {
		width:650px;
		border:2px solid #999999;
		height: 62px;
	    padding: 0 25px;
	}
	.bzxrname_input {
		
	}
	
	/* 身份证号码/组织机构代码 */
