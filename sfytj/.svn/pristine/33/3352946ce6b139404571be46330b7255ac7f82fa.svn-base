 /*
        播放视频新闻、图片新闻的方法
        news_array: 用于播放新闻的数组
        index: 当前播放到数组的第几个
        speed: 轮播速度，单位毫秒
    */
    function play_video_pic_news(news_array, index, speed){
        $(".news_container").html(news_array[num].content);
        $(".news_item").fadeIn(1000, function(){
            if(news_array[index].type==0){
                console.log("视频新闻");
                num++;
                if(num==news_array.length){
                    num=0;
                }
                play_video();
            }else{
                setTimeout(function(){
                    $(".news_item").fadeOut(1000, function(){
                        num++;
                        if(num==news_array.length){
                            num=0;
                        }
                        play_video_pic_news(news_array, num, 10000);
                    })
                }, speed);
            }
        });
    }

//播放视频
    function play_video(){
       var fullscreen_ele = $('.fullscreen_video');
       var video =  $(".news_item").find(".news_video");
       if(video.length>0){
            fullscreen_ele.attr("src", video[0].src);
            fullscreen_ele.show();
            fullscreen_ele[0].play();
       }
    }


