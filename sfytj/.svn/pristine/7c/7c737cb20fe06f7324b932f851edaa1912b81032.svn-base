/*填写查询密码*/
.yyyjyt{
  /* 导航条颜色的变化 */

  /*输入内容上面提示的几个字*/
  .tip{
    position: relative;
    top: 43px;
  }
  /*输入内容部分*/
  .input_list{
    /*字体样式*/
    position: relative;
    top: 65px;
    margin-left: auto;
    width: 75%;
    margin-right: auto;
    .title{
      float: left;
      line-height: 65px;
      color: #999;
    }

    /*申请人姓名*/
    .input_name {
      width: 75%;
      height: 65px;
      border: 1px solid #9e9e9e;
      padding: 20px 20px 20px 25px;
      margin-top: 15px;
    }

    /*登录方式左边类型*/
    .dlfsleft{
      float: left;
      width:20%;
      margin-top: 10px;
    }
    .dlfsright{
        float: left;
        width: 80%;
        height: 65px;
        /*margin-left: -9px;*/
    }
    /*登录方式*/
    .dlfs{
      margin-top: 35px;
      /*案件编号和手机号码样式*/
      .yuangao,.beigao{
        float: left;
        width: 28%;
        height: 65px;
	    line-height:65px;
        text-align:center;
        border: 1px solid #9e9e9e;
      }
      .beigao{
        margin-left: 5%;
      }
      .disanren{
         float: left;
         width: 28%;
         height: 65px;
	     line-height:65px;
	     text-align:center;
         border: 1px solid #9e9e9e;
         margin-left: 5%;
      }
      /*选中的样式*/
      .selected{
        border: 1px solid #53aee7; 
      }
     
    }
    /*申请流水号*/
    .sqlsh{
      margin-top: 35px;
      .input_sqyt{
        float: left;
        width: 75%;
        height: 165px;
        border: 1px solid #9e9e9e;
        overflow-y: scroll;
        padding: 20px 20px 20px 25px;
	resize: none;
      }
  

    }

    /*查询密码*/
    .cxmm{
      margin-top: 20px;
      .input_mm{
        float: left;
        width: 75%;
        height: 65px;
        border: 1px solid #9e9e9e;
        /*margin-left: 10px;*/
      }

    }

  }
 /*底部按钮样式*/

    .accept_btn{
      color: #fff;
      margin-right: 20px;
      position: relative;
      right: 802px;
