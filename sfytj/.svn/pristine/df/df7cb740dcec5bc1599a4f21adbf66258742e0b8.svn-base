<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>失信人查询结果详情表</title>
    <link rel="stylesheet" type="text/css" href="/sfytj/dist/styles/main.min.css"/>
    <link rel="stylesheet" type="text/css" href="/sfytj/bower_components/bootstrap/dist/css/bootstrap.min.css"/>
</head>
<!--失信人查询结果详情表-->
<body>
<div class="sxrcx_body">
    <!-- 失信人查询结果 -->
    <div class="sxrcxjgxq_body clearfix">
        <div class="sxrcxjgxq_body_left fl">
            <div class="sxrcxjgxq_item1">
                <div>
                    被执行人姓名/名称：
                    <span id="sxr_name" class="xqjg"></span>
                </div>
            </div>

            <div class="sxrcxjgxq_item">
                <div>
                    年龄：
                    <span id="sxr_age" class="xqjg"></span>
                </div>
            </div>

            <div class="sxrcxjgxq_item">
                <div>
                    执行法院：
                    <span id="sxr_fayuan" class="xqjg"></span>
                </div>
            </div>


            <div class="sxrcxjgxq_item">
                <div>
                    案号：
                    <span id="sxr_anhao" class="xqjg"></span>
                </div>
            </div>



            <div class="sxrcxjgxq_item">
                <div>失信被执行人行为具体情形：</div>
                <div class="sxr_jtqx xqjg" id="sxr_jtqx"></div>
            </div>


        </div>

        <div class="sxrcxjgxq_body_right fr">
            <div class="sxrcxjgxq_item1">
                <div>
                    性别：
                    <span id="sxr_sex" class="xqjg"></span>
                </div>
            </div>

            <div class="sxrcxjgxq_item">
                <div>
                    身份证号码/组织机构代码：
                    <span id="sxr_sfzhm" class="xqjg"></span>
                </div>
            </div>

            <div class="sxrcxjgxq_item">
                <div>
                    省份：
                    <span id="sxr_province" class="xqjg"></span>
                </div>
            </div>


            <div class="sxrcxjgxq_item">
                <div>
                    做出执行依据单位：
                    <span id="sxr_yjdw" class="xqjg"></span>
                </div>
            </div>

            <div class="sxrcxjgxq_item">
                <div>被执行人的履行情况：</div>
                <div class="sxr_lxqk xqjg" id="sxr_lxqk"></div>
            </div>

            <div class="sxrcxjgxq_item">
                <div>
                    发布时间：
                    <span id="sxr_fb_time" class="xqjg"></span>
                </div>
            </div>

        </div>
        <div class="clear"></div>
        <div class="sxrcxjgxq_item">
            <div>生效法律文书确定的义务：</div>
            <div class="sxr_yiwu xqjg" id="sxr_yiwu"></div>
        </div>

    </div>
</div>
<script type="text/javascript" src="/sfytj/bower_components/jquery/dist/jquery.min.js"></script>
<script type="text/javascript" src="/sfytj/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
<script type="text/javascript" src="/sfytj/dist/scripts/main.min.js"></script>
<script type="text/javascript">
    window.onload=function(){
//页面初始化，给各个失信人查询结果item赋值；
        var sxrxq_str = getCookie("sxrxq");
        var value = JSON.parse(sxrxq_str);

        console.log(value);

        if(value.iname!=undefined){document.getElementById('sxr_name').innerHTML = value.iname;}
        if(value.age!=undefined){document.getElementById('sxr_age').innerHTML = value.age;}
        if(value.courtName!=undefined){document.getElementById('sxr_fayuan').innerHTML = value.courtName;}
        if(value.caseCode!=undefined){document.getElementById('sxr_anhao').innerHTML = value.caseCode;}
        if(value.duty!=undefined){document.getElementById('sxr_yiwu').innerHTML = value.duty;}
        if(value.disruptTypeName!=undefined){document.getElementById('sxr_jtqx').innerHTML = value.disruptTypeName;}
        if(value.sexy!=undefined){document.getElementById('sxr_sex').innerHTML = value.sexy;}
        if(value.cardNum!=undefined){document.getElementById('sxr_sfzhm').innerHTML = value.cardNum;}
        if(value.areaName!=undefined){document.getElementById('sxr_province').innerHTML = value.areaName;}
        if(value.gistUnit!=undefined){document.getElementById('sxr_yjdw').innerHTML = value.gistUnit;}
        if(value.performance!=undefined){document.getElementById('sxr_lxqk').innerHTML = value.performance;}
        if(value.publishDate!=undefined){document.getElementById('sxr_fb_time').innerHTML = value.publishDate; }
    };

</script>

</body>
</html>